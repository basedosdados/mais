    "100 * SUM(nivel_1 * peso_aluno) / SUM(peso_aluno) as nivel_1,\n",
    "100 * SUM(nivel_2 * peso_aluno) / SUM(peso_aluno) as nivel_2,\n",
    "100 * SUM(nivel_3 * peso_aluno) / SUM(peso_aluno) as nivel_3,\n",
    "100 * SUM(nivel_4 * peso_aluno) / SUM(peso_aluno) as nivel_4,\n",
    "100 * SUM(nivel_5 * peso_aluno) / SUM(peso_aluno) as nivel_5,\n",
    "100 * SUM(nivel_6 * peso_aluno) / SUM(peso_aluno) as nivel_6,\n",
    "100 * SUM(nivel_7 * peso_aluno) / SUM(peso_aluno) as nivel_7,\n",
    "100 * SUM(nivel_8 * peso_aluno) / SUM(peso_aluno) as nivel_8,\n",
    "100 * SUM(nivel_9 * peso_aluno) / SUM(peso_aluno) as nivel_9,\n",
    "100 * SUM(nivel_10 * peso_aluno) / SUM(peso_aluno) as nivel_10\n",
    "\n",
    "FROM saeb\n",
    "GROUP BY ano, rede, localizacao, serie, disciplina, sigla_uf\n",
    "))\n",
    "PIVOT (\n",
    "MAX (media) AS media,\n",
    "MAX (nivel_0) AS nivel_0,\n",
    "MAX (nivel_1) AS nivel_1,\n",
    "MAX (nivel_2) AS nivel_2,\n",
    "MAX (nivel_3) AS nivel_3,\n",
    "MAX (nivel_4) AS nivel_4,\n",
    "MAX (nivel_5) AS nivel_5,\n",
    "MAX (nivel_6) AS nivel_6,\n",
    "MAX (nivel_7) AS nivel_7,\n",
    "MAX (nivel_8) AS nivel_8,\n",
    "MAX (nivel_9) AS nivel_9,\n",
    "MAX (nivel_10) AS nivel_10\n",
    "FOR disciplina IN\n",
    "('LP', 'MT')\n",
    ")\n",
    ")\n",
    "PIVOT (\n",
    "MAX (media_LP) AS media_LP,\n",
    "MAX (media_MT) AS media_MT,\n",
    "MAX (nivel_0_LP) AS nivel_0_LP,\n",
    "MAX (nivel_1_LP) AS nivel_1_LP,\n",
